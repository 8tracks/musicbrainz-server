    <td>[% stats.statistic('count.artist.type.null') %]</td>
    <td>[% l('{n}%', { n => round1(stats.ratio('count.artist.type.null', 'count.artist')) }) %]</td>
</tr>
<tr>
    <td>[% l('Artists:') %]</td>
